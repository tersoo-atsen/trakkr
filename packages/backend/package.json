{
  "name": "backend",
  "version": "1.0.0",
  "description": "Application backend",
  "main": "server.js",
  "scripts": {
    "build": "rm -rf ./build && npx babel src -d build",
    "dev": "nodemon --exec npx babel-node src/server.js",
    "lint": "npx eslint --fix src/*.js",
    "db-init": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "db-migrate": "sequelize db:migrate",
    "db-undo-migrate": "sequelize db:migrate:undo:all",
    "db-seed": "sequelize db:seed:all",
    "test": "cross-env NODE_ENV=test yarn db-init && jest --config jest.config.js",
    "test:coverage": "cross-env NODE_ENV=test yarn db-init && jest --config jest.config.js --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.14.2",
    "apollo-server-express": "^2.14.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.25.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-resolvers": "^0.3.3",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.5.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.8.4",
    "axios": "^0.21.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "graphql-tools": "^4.0.6",
    "jest": "^24.9.0",
    "node-fetch": "^2.6.1",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2"
  }
}
